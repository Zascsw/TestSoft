@model Dictionary<string, object>

<h1>Данные из таблицы</h1>


<table>
    <thead>
        <tr>
            @foreach (var columnName in Model.Keys)
            {
                <th>@columnName</th>
            }
        </tr>
    </thead>
    <tbody>
        <tr>
            @foreach (var columnValue in Model.Values)
            {
                <td>@columnValue</td>
            }
        </tr>
    </tbody>
</table>
